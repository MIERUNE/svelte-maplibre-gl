{
	"$schema": "https://turbo.build/schema.json",
	"ui": "tui",
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": ["dist/**", ".svelte-kit/**"],
			"cache": true
		},
		"prepack": {
			"dependsOn": ["^prepack"],
			"outputs": ["dist/**"],
			"cache": true
		},
		"check": {
			"dependsOn": ["^build", "^check"]
		},
		"check:watch": {},
		"lint": {},
		"test": {},
		"test:e2e": {},
		"preview": {}
	}
}
