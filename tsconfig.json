{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"allowJs": true,
		"checkJs": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": true,
		// "module": "NodeNext",
		"moduleResolution": "bundler",
		"types": ["vite-plugin-svelte-docgen"]
	},
	"exclude": ["node_modules", "dist", "**/dist", "extensions/*/dist", "svelte-maplibre-gl/dist"]
}
